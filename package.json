  "dependencies": {
    "@jwalsh/jsonwebtoken": "^7.2.2",
    "blockstack-auth": "^0.2.1",
    "express": "^4.14.0",
    "got": "^6.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-latest": "^6.16.0",
    "http-server": "^0.9.0",
    "nodemon": "^1.11.0"
  }
}
